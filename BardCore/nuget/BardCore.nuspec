<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>BardCore</id>
        <version>$Version$</version>
        <title>BardCore - A Math Library</title>
        <authors>Bardio</authors>
        <readme>docs\README.md</readme>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <projectUrl>https://github.com/BardoBard/BardCore/</projectUrl>
        <description>Bardcore is a math library mainly used for raytracing.</description>
        <releaseNotes>$ReleaseNotes$</releaseNotes>
        <copyright>$copyright$</copyright>
    </metadata>
    <files>
        <file src="..\..\README.md" target="docs\"/>
        <file src="..\RELEASE_NOTES" target="docs\"/>
        <file src="..\docs\*.md" target="docs\"/>
        <file src="..\licenses\LICENSE.txt" target="licenses\"/>

        <file src="..\include\**\*.*" target="build\native\include\"/>
        <file src="BardCore.targets" target="build\native"/>

        <file src="..\..\out\Win32\Debug\BardCore.lib" target="build\native\lib\x86\debug\"/>
        <file src="..\..\out\Win32\Release\BardCore.lib" target="build\native\lib\x86\release\"/>
        <file src="..\..\out\x64\Debug\BardCore.lib" target="build\native\lib\x64\debug\"/>
        <file src="..\..\out\x64\Release\BardCore.lib" target="build\native\lib\x64\release\"/>
    </files>
</package>